{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "syncer",
      "type": "debugpy",
      "request": "launch",
      "justMyCode": false,
      "cwd": "${workspaceFolder}/syncer",
      "program": "syncer/main.py",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "go-syncer",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/go-syncer/cmd/syncer",
      "envFile": "${workspaceFolder}/go-syncer/.env"
    },
    {
      "name": "tmp",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/go-syncer/tmp",
      "envFile": "${workspaceFolder}/go-syncer/.env"
    },
  ]
}
